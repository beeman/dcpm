{
  "name": "@dcpm/cli",
  "version": "1.3.0",
  "description": "The cli for dcpm",
  "author": "Harris Weeks <root@hweeks.com>",
  "homepage": "",
  "license": "ISC",
  "bin": {
    "dcpm": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": "git@github.com:hweeks/dcpm.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "watch": "tsc -w",
    "watch:brk": "TS_NODE_FILES=true node --inspect-brk -r ts-node/register ./lib/index.ts",
    "dev:start": "nodemon",
    "semantic-release": "semantic-release -e hweeks-semantic-release-monorepo",
    "lint": "eslint lib/**/*.ts"
  },
  "dependencies": {
    "@dcpm/api": "1.3.0",
    "archiver": "^3.1.1",
    "chalk": "^3.0.0",
    "form-data": "^3.0.0",
    "js-yaml": "^3.13.1",
    "semver": "^7.1.1",
    "unzipper": "^0.10.5",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/npm": "^5.3.4",
    "@types/archiver": "^3.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.12.14",
    "@types/unzipper": "^0.10.1",
    "@types/yargs": "^13.0.3",
    "hweeks-semantic-release-monorepo": "^1.0.0",
    "nodemon": "^2.0.1",
    "semantic-release": "^15.13.31",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
