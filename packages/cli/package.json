{
  "name": "@dcpm/cli",
  "version": "0.0.0",
  "description": "The cli for dcpm",
  "author": "Harris Weeks <root@hweeks.com>",
  "homepage": "",
  "license": "ISC",
  "bin": "dist/cli/index.js",
  "main": "dist/api/index.js",
  "types": "dist/api/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "watch": "tsc -w",
    "watch:brk": "TS_NODE_FILES=true node --inspect-brk -r ts-node/register",
    "dev:start": "nodemon",
    "lint": "eslint lib/**/*.ts"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "form-data": "^3.0.0",
    "js-yaml": "^3.13.1",
    "node-fetch": "^2.6.0",
    "typescript": "^3.7.2",
    "unzipper": "^0.10.5",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/archiver": "^3.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.12.14",
    "@types/node-fetch": "^2.5.3",
    "@types/unzipper": "^0.10.1",
    "@types/yargs": "^13.0.3",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.2"
  }
}
